{
  "name": "ngx-graphs-d3",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sushant-kum/ngx-d3-graphs"
  },
  "homepage": "https://ngx-graphs-d3.sushantk.com",
  "license": "MIT",
  "author": {
    "name": "Sushant Kumar",
    "email": "sushant.kum96@gmail.com",
    "url": "https://sushantk.com"
  },
  "keywords": [
    "d3",
    "graphs",
    "graph",
    "charts",
    "chart",
    "angular",
    "data visualization",
    "d3-visualization",
    "d3js",
    "documentation"
  ],
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run build:dev:lib\" \"sleep 20 && npm run serve:doc\" \"npm run mock-server:dev\"",
    "test": "ng test",
    "test:CI": "ng test --browsers ChromeHeadless --no-watch --code-coverage",
    "e2e": "ng e2e",
    "serve:doc": "ng serve --host 0.0.0.0 --disableHostCheck --proxy-config proxy.config.json",
    "mock-server:dev": "nodemon --config \"./nodemon.json\"/",
    "build:dev:lib": "ng build --project ngx-graphs-d3 --watch",
    "build:prod:lib": "ng build --prod --project ngx-graphs-d3",
    "predeploy:prod:lib": "npm run build:prod:lib",
    "deploy:prod:lib": "npm publish dist/ngx-graphs-d3",
    "build:prod:doc": "npm run compodoc:lib && ng build --prod --project ngx-graphs-d3-doc",
    "predeploy:prod:doc": "npm run build:prod:doc",
    "deploy:prod:doc": "firebase deploy --only hosting:prod",
    "predeploy:testbed:doc": "npm run build:prod:doc",
    "deploy:testbed:doc": "firebase deploy --only hosting:testbed",
    "compodoc:lib": "compodoc --config ./.compodocrc.lib.json",
    "lint:fix": "npm run lint:fix:lib && npm run lint:fix:doc",
    "lint:fix:lib": "ng lint ngx-graphs-d3 --fix",
    "lint:fix:doc": "ng lint ngx-graphs-d3-doc --fix",
    "lint:check": "ng lint",
    "format": "npm run format:lib && npm run format:doc",
    "format:lib": "prettier \"projects/ngx-graphs-d3/src/**/*.{css,html,js,json,md,scss,ts}\"",
    "format:doc": "prettier \"projects/ngx-graphs-d3-doc/src/**/*.{css,html,js,json,md,scss,ts}\"",
    "format:fix": "npm run format:fix:lib && npm run format:fix:doc",
    "format:fix:lib": "prettier --write \"projects/ngx-graphs-d3/src/**/*.{css,html,js,json,md,scss,ts}\"",
    "format:fix:doc": "prettier --write \"projects/ngx-graphs-d3-doc/src/**/*.{css,html,js,json,md,scss,ts}\"",
    "format:check": "npm run format:check:lib && npm run format:check:doc",
    "format:check:lib": "prettier --check \"projects/ngx-graphs-d3/src/**/*.{css,html,js,json,md,scss,ts}\"",
    "format:check:doc": "prettier --check \"projects/ngx-graphs-d3-doc/src/**/*.{css,html,js,json,md,scss,ts}\"",
    "firebase:target:apply:prod": "firebase target:apply hosting prod ngx-graphs-d3",
    "firebase:target:apply:testbed": "firebase target:apply hosting testbed ngx-graphs-d3-testbed"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.5",
    "@angular/cdk": "^9.1.1",
    "@angular/common": "^9.0.5",
    "@angular/compiler": "^9.0.5",
    "@angular/core": "^9.0.5",
    "@angular/forms": "^9.0.5",
    "@angular/material": "^9.1.1",
    "@angular/platform-browser": "^9.0.5",
    "@angular/platform-browser-dynamic": "^9.0.5",
    "@angular/router": "^9.0.5",
    "@fortawesome/angular-fontawesome": "^0.6.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "assign-deep": "^1.0.1",
    "css-element-queries": "^1.2.3",
    "d3": "^5.15.0",
    "d3-svg-legend": "^2.25.6",
    "fast-deep-equal": "^3.1.1",
    "google-palette": "^1.1.0",
    "moment": "^2.24.0",
    "object-assign-deep": "^0.4.0",
    "prismjs": "^1.19.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.11.1",
    "unique-colors": "^1.0.1",
    "url-parse": "^1.4.7",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.7",
    "@angular-devkit/build-ng-packagr": "^0.900.7",
    "@angular/cli": "^9.0.7",
    "@angular/compiler-cli": "^9.0.7",
    "@angular/language-service": "^9.0.7",
    "@compodoc/compodoc": "^1.1.11",
    "@types/d3": "^5.7.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/json-server": "^0.14.2",
    "@types/node": "^12.12.30",
    "@types/object-assign-deep": "^0.4.0",
    "@types/prismjs": "^1.16.0",
    "@types/url-parse": "^1.4.3",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.1.0",
    "connect-pause": "^0.1.0",
    "firebase-tools": "^7.15.1",
    "gzipper": "^3.5.0",
    "husky": "^3.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "^0.15.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "ng-packagr": "^9.0.3",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "prettier-tslint": "^0.4.2",
    "pretty-quick": "^2.0.1",
    "protractor": "^5.4.3",
    "scss-bundle": "^3.1.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}

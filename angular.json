{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-graphs-d3": {
      "projectType": "library",
      "root": "projects/ngx-graphs-d3",
      "sourceRoot": "projects/ngx-graphs-d3/src",
      "prefix": "ngx-d3",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ngx-graphs-d3/tsconfig.lib.json",
            "project": "projects/ngx-graphs-d3/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-graphs-d3/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-graphs-d3/src/test.ts",
            "tsConfig": "projects/ngx-graphs-d3/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-graphs-d3/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["projects/ngx-graphs-d3/tsconfig.lib.json", "projects/ngx-graphs-d3/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "ngx-graphs-d3-doc": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/ngx-graphs-d3-doc",
      "sourceRoot": "projects/ngx-graphs-d3-doc/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ngx-graphs-d3-doc",
            "index": "projects/ngx-graphs-d3-doc/src/index.html",
            "main": "projects/ngx-graphs-d3-doc/src/main.ts",
            "polyfills": "projects/ngx-graphs-d3-doc/src/polyfills.ts",
            "tsConfig": "projects/ngx-graphs-d3-doc/tsconfig.app.json",
            "aot": true,
            "assets": ["projects/ngx-graphs-d3-doc/src/assets"],
            "styles": [
              "projects/ngx-graphs-d3-doc/src/assets/libraries/w3.css/4/w3.css",
              "projects/ngx-graphs-d3-doc/src/assets/libraries/hamburgers/hamburgers.min.css",
              "projects/ngx-graphs-d3-doc/src/styles/index.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/ngx-graphs-d3-doc/src/environments/environment.ts",
                  "with": "projects/ngx-graphs-d3-doc/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ngx-graphs-d3-doc:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ngx-graphs-d3-doc:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngx-graphs-d3-doc:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-graphs-d3-doc/src/test.ts",
            "polyfills": "projects/ngx-graphs-d3-doc/src/polyfills.ts",
            "tsConfig": "projects/ngx-graphs-d3-doc/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-graphs-d3-doc/karma.conf.js",
            "assets": ["projects/ngx-graphs-d3-doc/src/assets"],
            "styles": [
              "projects/ngx-graphs-d3-doc/src/assets/libraries/w3.css/4/w3.css",
              "projects/ngx-graphs-d3-doc/src/assets/libraries/hamburgers/hamburgers.min.css",
              "projects/ngx-graphs-d3-doc/src/styles/index.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngx-graphs-d3-doc/tsconfig.app.json",
              "projects/ngx-graphs-d3-doc/tsconfig.spec.json",
              "projects/ngx-graphs-d3-doc/e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/ngx-graphs-d3-doc/e2e/protractor.conf.js",
            "devServerTarget": "ngx-graphs-d3-doc:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ngx-graphs-d3-doc:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "ngx-graphs-d3"
}
